{
  "name": "@lunaticsatoshi/lor-sdk-test",
  "version": "1.0.3",
  "description": "A sdk for the lord of the rings api",
  "keywords": [
    "lor",
    "lor-api",
    "rest",
    "one-api"
  ],
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.mjs"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "rm -rf dist && microbundle --tsconfig tsconfig.json --no-sourcemap",
    "prebuild": "yarn clean",
    "build:watch": "tsc --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "clean": "rm -rf dist"
  },
  "author": "Lunaticsatoshi",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "isomorphic-unfetch": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@types/jest": "^29.4.0",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "microbundle": "^0.15.1",
    "nock": "^13.3.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lunaticsatoshi/lor-sdk-test.git"
  },
  "homepage": "https://github.com/Lunaticsatoshi/lor-sdk-test#readme"
}
